{
  "name": "karaagac-fatih-web",
  "version": "1.0.0",
  "description": "Karaağaç Fatih Web Application",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist-app",
    "build:web": "npm run clean && copyfiles -u 0 \"*.html\" \"css/**\" \"img/**\" \"js/**\" \"admin/**\" \"diger/**\" \"kermes/**\" \"muhasebe/**\" \"nehari/**\" \"parcalar/**\" \"personel/**\" \"talebe/**\" dist-app",
    "sync:ios": "npx cap sync ios",
    "open:ios": "npx cap open ios",
    "ios": "npm run build:web && npm run sync:ios && npm run open:ios"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samettckrr/karaagac-fatih-web.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@capacitor/core": "^7.4.2",
    "@capacitor/ios": "^7.4.2"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.4.2",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.0.1"
  }
}
